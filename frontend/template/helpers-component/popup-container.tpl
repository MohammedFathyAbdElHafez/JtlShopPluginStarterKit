{block name='popup-container'}
    <div class="pop-up-container" id="pop-up-container">
        <div class="pop-up-content-parent">
            <div class="pop-up-content" id="pop-up-content">
                <i id="close-rental-car-popup" class="fa fa-times">edit</i>

                <img src='./exclamation-mark.png' class='pop-up-icon' id="pop-up-icon">

                <p class="pop-up-message" id='error-message-v2'>
                    Lorem ipsum dolor sit amet consectetur adipisicing elit. Voluptas, cumque! Sunt sapiente animi
                </p>

                <div class="button-container">
                    <button class="pop-up-button" id="confirm-pop-up-screen">yes</button>
                    <button class="pop-up-button" id="close-pop-up-screen">no</button>
                </div>

            </div>
        </div>
    </div>
{/block}